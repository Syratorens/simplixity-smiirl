# Facebook System User Access Token (n'expire jamais)
FACEBOOK_SYSTEM_USER_ACCESS_TOKEN=votre_system_user_access_token
# Automatically retrieved on first launch
INSTAGRAM_BUSINESS_ACCOUNT_ID=

# Instagram Configuration (optionnel, pour API v1 non-officielle)
INSTAGRAM_USERNAME=votre_username_instagram

# Service à utiliser (instagram, etc.)
SERVICE=instagram

# Configuration serveur
PORT=9000

# Durée de vie du cache en secondes (cache PHP fichier)
# penser à mettre la même valeur dans le cache serveur nginx default.conf
CACHE_LIFETIME=120

# User et Group ID pour les permissions Docker
LOCAL_UID=501
LOCAL_GID=20